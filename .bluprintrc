{
  "bluprint": "^0.0.19",
  "name": "graphics rig",
  "category": "",
  "actions": [{
    "action": "prompt",
    "questions": [{
      "type": "text",
      "name": "projectName",
      "message": "What should we call this project?"
    }]
  }, {
    "action": "remove",
    "paths": ["README.md"]
  }, {
    "action": "move",
    "paths": ["PROJECT_README.md", "README.md"]
  }, {
    "action": "render",
    "engine": "mustache",
    "files": ["README.md"]
  }, {
    "action": "regexreplace",
    "files": ["package.json"],
    "replace": [
      ["projectName", "{{#slugify}}{{ projectName }}{{/slugify}}"]
    ]
  }, {
    "action": "execute",
    "cmds": [
      ["yarn"],
      ["runner", ["check:ai2html"]],
      ["git", ["init"]],
      ["git", ["add", "."]],
      ["git", ["commit", "-m", "initial"]],
      ["runner", ["check:creds"]],
      ["runner", ["start"]]
    ]
  }]
}
